{
  "name": "@torq/get-strava-activity-signals",
  "version": "1.0.0",
  "description": "Extracts Strava activity signals from a given Strava activity.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://github.com/torqlab/get-strava-activity-signals#readme",
  "engines": {
    "node": "24.x"
  },
  "bugs": {
    "url": "https://github.com/torqlab/get-strava-activity-signals/issues"
  },
  "keywords": [
    "strava"
  ],
  "scripts": {
    "build:types": "bunx tsc -p tsconfig.build.json",
    "build:esm": "bun build ./index.ts --outfile dist/index.mjs --target node",
    "build:cjs": "bun build ./index.ts --outfile dist/index.cjs --target node --format cjs",
    "build": "bun run clean && bun run build:types && bun run build:esm && bun run build:cjs",
    "test": "bun test",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint packages --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "prepublishOnly": "bun run build",
    "clean": "rm -rf dist"
  },
  "author": {
    "name": "Bohdan Balov",
    "email": "balovbohdan@gmail.com",
    "url": "https://balov.dev"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/bun": "^1.3.8",
    "bun": "^1.3.6",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsdoc": "^48.10.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.15.0"
  }
}
